<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="false" android:state_focused="false"
        android:state_pressed="true" >
        <shape>
            <solid android:color="#7A4900"/>
            <stroke
                android:width="0.1dp"
                android:color="#cccccc" />
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#CC7A00"/>
            <stroke
                android:width="0.1dp"
                android:color="#cccccc" />
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp" />
        </shape>
    </item>
</selector>